      <div class="card m-3 pastlogs" style="width: 18rem;">
        <div class="card-header">
          
          <div class="row mt-4">
<section class="box">
  <div class="col-md-6">
    <h3>Create a New exercise:</h3>
    <form class="form new-workout-form">
      <div class="form-group">
        <label for="exercise-name">Exercise Name:</label>
        <input class="form-input" type="text" id="exercise-name" name="exercise-name" />
      </div>
      <div class="form-group">
        <label for="exercise-weight">Exercise Weight ($):</label>
        <input class="form-input" type="number" id="exercise-weight" name="exercise-weight" />
      </div>
      <div class="form-group">
        <label for="exercise-reps">Exercise Reps ($):</label>
        <input class="form-input" type="number" id="exercise-reps" name="exercise-reps" />
      </div>
      <div class="form-group">
        <label for="exercise-duration">Exercise Duration ($):</label>
        <input class="form-input" type="number" id="exercise-duration" name="exercise-duration" />
      </div>
      <div class="form-group">
        <button type="submit" id="category-btn" class="btn btn-primary">Create</button>
      </div>
    </form>
  </div>
  <div class="col-md-6">
    {{#each exercises as |exercise|}}
    <div class="row mb-2">
      <div class="col-md-8">
        <h4><a href="/project/{{exercise.id}}">{{exercise.exercise_name}}</a></h4>
      </div>
      <div class="col-md-4">
        <button class="btn btn-sm btn-danger" data-id="{{exercise.id}}">DELETE</button>
      </div>
    </div>
    {{/each}}
  </div>
</section>
