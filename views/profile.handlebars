<div>
  <h3> Welcome, {{user_name}} </h3>
</div>
<div class="container exercise-list">
  {{#if exercises}}
  <div style="display: flex; height: 600px; overflow: scroll; margin: 30px; border-radius:30px;">
    <div class="logsbackground" > <span class="text-center" style="font-size: 40px; color:rgb(167, 167, 167);">Logs</span>
      {{#each exercises as |exercise|}}
      <div class="text-center eachlog" href="/workout/{{exercise.id}}">
        <small>{{exercise.duration}}min</small>
        <div class="separator"></div> <a href="/workout/{{exercise.id}}">{{exercise.exercise_name}}</a> 
     
      {{!-- <div class="d-grid gap-2 col-12 m-0 mx-auto"> --}}
        <button class="btn btn-sm btn-danger" id="logbutton" data-id="{{exercise.id}}">DELETE</button>
      {{!-- </div> --}}
       </div>
      {{/each}}
    </div>
  </div>
  {{/if}}
  <div style="display: flex; padding: 200px; height: 10px; ">
    <a role="button" href="/exerciseform"><span class="material-symbols-outlined text-center"
        style="color: rgb(0, 0, 0); font-size: 100px;" id="addicon">
        add
      </span></a>
  </div>
</div>

</div>

<script src="/js/profile.js"></script>